#include <iostream>
using namespace std;

int main() {
    int i = 1, num, sum = 0;

    cout << "Enter 10 numbers: ";

    do {
        cin >> num;
        sum += num;   // add input to sum
        i++;
    } while(i <= 10);

    cout << "Sum of all numbers: " << sum;

    return 0;
}
